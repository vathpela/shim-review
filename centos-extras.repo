[centos-extras]
name=CentOS-$releasever - Extras
baseurl=https://vault.centos.org/7.2.1511/extras/$basearch
#baseurl=http://mirror.centos.org/centos/$releasever/extras/$basearch/os/
failovermethod=priority
gpgcheck=0
skip_if_unavailable=False
exclude=gcc* binutils* git* annobin* elfutils* *rpm* glibc* efi* redhat* make* diff* git* patch* libgcc* libffi* glibc* xz* libcom* libselinux* zlib*
